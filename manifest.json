{
  "manifest_version": 2,
  "name": "YTM URL Auto Sender (Firefox)",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "webNavigation",
    "https://music.youtube.com/*",
    "http://127.0.0.1:8787/*",
    "http://localhost:8787/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
